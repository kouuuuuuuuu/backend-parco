<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.Eparking.dao.PriceMethodMapper">

    <insert id="create" parameterType="com.project.Eparking.domain.PriceMethod">
        INSERT INTO
            pricemethodreservation
        (
            reservationID,
            method1,
            method2,
            method3
        )
        VALUES
        (
            #{reservationID},
            #{method1},
            #{method2},
            #{method3}
        );
    </insert>
</mapper>