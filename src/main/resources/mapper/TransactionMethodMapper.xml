<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.Eparking.dao.TransactionMethodMapper">

   <select id="getListTransactionMethodByHistoryId" resultType="com.project.Eparking.domain.TransactionMethod">
       SELECT
           transactionMethodID,
           historyID,
           bankCode,
           bankName,
           bankNumber
       FROM
            transactionmethod
       WHERE
        historyID = #{historyID};
   </select>

</mapper>