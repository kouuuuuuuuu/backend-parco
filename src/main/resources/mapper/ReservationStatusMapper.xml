<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.Eparking.dao.ReservationStatusMapper">
    <select id="getReservationStatusByID" resultType="com.project.Eparking.domain.ReservationStatus">
        SELECT
            statusID,
            statusName
        FROM
            reservationstatus
        WHERE
            statusID = #{status};
    </select>

</mapper>